<!--pages/papers/papers.wxml-->
<view>
  <view>Slip to change year and season:</view>
  <picker-view indicator-style="height: 50px;" style="width: 100%; height: 50px;" value="{{ value }}" bindchange="bindChange">
    <picker-view-column>
      <view wx:for="{{ years }}" wx:key="*this" style="line-height: 50px">{{ year }}</view>
    </picker-view-column>
    <picker-view-column>
      <view wx:for="{{ seasons }}" wx:key="*this" style="line-height: 50px">{{ season }}</view>
    </picker-view-column>
  </picker-view>
</view>
